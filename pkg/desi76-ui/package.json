{
	"name": "desi76-ui",
	"description": "sveltekit-ui for desi76",
	"version": "0.1.9",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/parame76.git"
	},
	"homepage": "https://charlyoleg2.github.io/parame76/",
	"author": "charlyoleg",
	"license": "ISC",
	"keywords": [
		"parametrix",
		"3D",
		"CAD"
	],
	"type": "module",
	"scripts": {
		"cp_pgdsvg": "fse copy ../../node_modules/desi76/dist/pgdsvg static/pgdsvg",
		"prebuild": "svelte-kit sync",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"ci": "run-s cp_pgdsvg build lint check test:unit:once",
		"format": "prettier --write .",
		"github-pages-fix": "touch build/.nojekyll",
		"clean": "rimraf .svelte-kit build node_modules static/pgdsvg"
	},
	"dependencies": {
		"desi76": "^0.1.8",
		"geometrix": "^1.0.26",
		"geomui": "^1.0.26"
	},
	"devDependencies": {
		"@atao60/fse-cli": "^0.1.9",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/eslint": "^9.6.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "=3.11.0",
		"globals": "^16.5.0",
		"npm-run-all2": "^8.0.4",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"rimraf": "^6.1.0",
		"sass": "^1.93.3",
		"svelte": "^5.43.5",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3",
		"vite": "^7.2.2",
		"vitest": "^4.0.8"
	}
}
